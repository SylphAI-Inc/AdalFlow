        "from adalflow.utils.logger import get_logger\n",
        "\n",
        "app_logger = get_logger(name=\"my_app\", level=\"DEBUG\", save_dir=\"./logs\") # log to ./logs/my_app.log\n",
        "\n",
        "class Task:\n",
        "    def __init__(self):\n",
        "        app_logger.info(\"This is a user program child logger\")"
